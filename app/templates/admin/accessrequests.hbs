<h1>Access Requests</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>ID</th>
        <th>E-mail</th>
    </tr>
    </thead>
    <tbody>
    {{#each model as |accessrequest|}}
        <tr>
            <th>{{accessrequest.id}}</th>
            <td>{{accessrequest.email}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>